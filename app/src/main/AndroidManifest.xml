<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.parkingv2">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launch_gct_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launch_gct_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".listview"/>
        <activity android:name=".MapsActivity" />
        <activity android:name=".VerifyPhoneActivity" />
        <activity android:name=".beforeLogin">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ReservationActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".SignupActivity" />
        <activity android:name=".ResetPasswordActivity" />
    </application>

</manifest>